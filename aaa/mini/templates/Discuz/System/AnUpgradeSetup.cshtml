@{
    ViewBag.Title = "AnUpgradeSetup";
    Layout = "~/Views/Templates/Discuz/Shared/_SystemLayout.cshtml";
}

<div class="road">
    <span class="suninfo_str">首页 > 系统设置 > 升级系统</span>
</div>
<div class="rtgy-box">
    <div class="rtgy-cont">
        <h3>升级系统</h3>
        <div class="set">
            <h5>升级系统</h5>
            <div class="group clear">
                <label class="suninfo_cont_str" ns="SysUpgrade">升级包：</label>
                <div class="file-box">
                    <form action="" method="post" enctype="multipart/form-data">
                        <input name="imgfile" type="file" id="imgfile" size="40" onchange="viewmypic(showimg,this.form.imgfile);">
                    </form>
                </div>
                <span class="msg suninfo_cont_str" ns="SysUpgrade">升级包名称必须以"mininas-"开始，以".tar.gz"结束</span>
            </div>
        </div>
        <div class="btnBox">
            <a class="save">保存</a>
        </div>
    </div>
</div>
@{
    ViewBag.Title = "InsideEditDiskCoupon";
    Layout = "~/Views/Templates/Discuz/Shared/_SystemLayout.cshtml";
}

<div class="road">
    <span class="suninfo_str">首页 > 存储设置 > 磁盘卷管理</span>
</div>
<div class="rtgy-box">
    <div class="rtgy-cont">
        <h3>磁盘卷管理</h3>
        <div class="tab_contBox">
            <div class="set">
                <h5>基础设置</h5>
                <div class="group clear postion_box">
                    <label class="suninfo_cont_str">磁盘卷名称：</label>
                    <input type="text" class="w200" value="">
                    <span class="msg"></span>
                </div>
                <div class="group clear">
                    <label class="suninfo_cont_str">物理空间：</label>
                    <input type="text" class="w200" value="">
                    <span class="msg"></span>
                </div>
                <div class="group clear">
                    <label class="suninfo_cont_str">已用：</label>
                    <input type="text" class="w200" value="">
                    <span class="msg"></span>
                </div>
                <div class="group clear">
                    <label class="suninfo_cont_str">有效：</label>
                    <input type="text" class="w200" value="">
                    <span class="msg"></span>
                </div>
            </div>
            <div class="set">
                <h5>高级设置</h5>
                <div class="group clear">
                    <label class="suninfo_cont_str">容量：</label>
                    <input type="text" class="w200" value="">
                    <span class="msg"></span>
                </div>
                <div class="group clear">
                    <label class="suninfo_cont_str">状态：</label>
                    <input type="text" class="w200" value="">
                    <span class="msg"></span>
                </div>
            </div>
            <div class="btnBox">
                <a class="save">保存</a>
                <a class="cancel">取消</a>
            </div>
        </div>
    </div>
</div>
